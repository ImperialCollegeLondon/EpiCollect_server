application: epicollectserver
version: 138
runtime: python
api_version: 1

handlers:
- url: /static
  static_dir: static

- url: /images
  static_dir: images

- url: /login.html
  static_files: login.html
  upload: login.html

- url: /login-safari.html
  static_files: login-safari.html
  upload: login-safari.html

- url: /login-successful.html
  static_files: login-successful.html
  upload: login-successful.html

- url: /create-project.html
  static_files: create-project.html
  upload: create-project.html

- url: /project.html
  static_files: project.html
  upload: project.html

- url: /ProjectDescription.html
  static_files: ProjectDescription.html
  upload: ProjectDescription.html

- url: /createOrEditForm.html
  static_files: createOrEditForm.html
  upload: createOrEditForm.html

- url: /checkboxgrouptest.html
  static_files: checkboxgrouptest.html
  upload: checkboxgrouptest.html


- url: /zipme
  script: zipme.py

- url: /test.*
  script: gaeunit.py

- url: /ecLogin
  script: main.py
  login: required
  
- url: /feedback
  script: main.py
  login: required

- url: .*
  script: main.py